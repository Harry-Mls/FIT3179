{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 800,
  "height": 450,
	"title": "EGM expenditure per person for each LGA",
	"projection": {
    "type": "conicEqualArea",
    "parallels": [
      -36,
      -38
    ],
    "rotate": [
      -145,
      0,
      0
    ]
  },
	"data": {
    "url": "://raw.githubusercontent.com/Harry-Mls/FIT3179/main/Data/VICLGAtopojson.json",
    "format": { "type": "topojson", "feature": "VicLGA"}
    },
    "mark": {"type": "geoshape", "fill": "lightgray", "stroke": "white"}
  },
  "transform": [
    {
      "lookup": "properties.NAME",
      "from": {
        "data": {
          "url": "https://raw.githubusercontent.com/FIT3179/Vega-Lite/main/3_choropleth_map/data/covid_10_10_2020.csv"
        },
        "key": "LGA Name",
        "fields": ["Active"]
      }
    },
  ]
  {
      "data": {
        "url": "https://raw.githubusercontent.com/FIT3179/Harry-Mls/main/data/EGM2023-2024.csv"
      },
  }

}